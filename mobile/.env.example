# Fintrax Mobile App Environment Variables
# Copy this file to .env for local development and fill in the values

# API Configuration
# For emulator/simulator: http://localhost:80/api
# For physical devices: http://YOUR_LOCAL_IP:80/api (e.g., http://192.168.1.100:80/api)
EXPO_PUBLIC_API_URL=http://localhost:80/]

# App Configuration
EXPO_PUBLIC_APP_NAME=Fintrax
EXPO_PUBLIC_APP_VERSION=1.0.0

# Environment
NODE_ENV=development
